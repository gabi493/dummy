{% extends '::base.html.twig' %}

{% block stylesheets %}
    <link href="{{ asset('css/loginFormTest.css') }}" type="text/css" rel="stylesheet" xmlns="http://www.w3.org/1999/html"
          xmlns="http://www.w3.org/1999/html">
{% endblock %}

    {% block body %}
        <div>
            <label class="title">Login Form Test:</label>
        </div>
        {% if output is defined  %}
            {# codigo html con el aspecto del error > #}
            <div class="text">
                <message id="message">{{ output.date }}</message>
                <br/>
                {#<message id="message">{{ output.route }}</message>#}
                {#<br/>#}
                <br/>
                {#{{ dump(output.salida) }}#}
                {{ output.salida|raw }}
            </div>
        {% endif %}
    {% endblock %}
